@model List<StudyPoint.Models.Course>
@{
    ViewBag.Title = "Download";
    Layout = "~/Views/Shared/UserSharedView.cshtml";
}
<br />
<h2>Download Study Material</h2>
<br />
<table class="table table-hover table-bordered">
    <tr>
        <th>Course Name</th>
        <th>Category</th>
    </tr>
    @foreach(var course in Model)
    {
        <tr>
            <td>@Html.ActionLink(course.Name, "Details", "User", new { id = course.Id}, null)</td>
            <td>@course.Category</td>
        </tr>
    }
</table>
